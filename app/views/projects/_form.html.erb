<%= form_for @project do |f| %>
<%= render "shared/error_messages", :target => @project %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name, required: :true %>
  </p>
 
  <p>
    <%= f.label :desciption %><br>
    <%= f.text_area :description, required: :true %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>